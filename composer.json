{
    "name": "tahaghafuri/faker",
    "type": "library",
    "description": "Faker is a PHP library that generates fake data for you.",
    "keywords": [
        "faker",
        "fixtures",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Taha Amin Ghafuri"
        }
    ],
    "require": {
        "php": "<=8.0"
    },
    "require-dev": {
        "ext-intl": "*",
        "phpunit/phpunit": "^*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Faker\\": "src/Faker/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
